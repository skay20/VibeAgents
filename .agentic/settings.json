      "evaluate_all_agents": true,
      "catalog": [
        "god_orchestrator",
        "intent_translator",
        "context_curator",
        "stack_advisor",
        "architect",
        "planner",
        "implementer",
        "qa_reviewer",
        "security_reviewer",
        "docs_writer",
        "release_manager",
        "repo_maintainer",
        "template_librarian",
        "migration_manager"
      ],
      "always_required_agents": [
        "architect",
        "qa_reviewer",
        "docs_writer"
      ],
      "signal_weights": {
