      "auto_generate_project_readme": true,
      "require_project_runbook_when_project_detected": true,
      "require_project_readme_when_project_detected": true,
      "update_readme_each_iteration": true,
      "enforce_parent_docs_each_iteration": true,
      "parent_docs_required_paths": [
        "README.md",
        "docs/QUICKSTART.md"
      ]
    },
    "prompt_resolution": {
      "default_version": "v1",
      "pilot_v2_enabled": true,
      "pilot_agents": [
        "god_orchestrator",
        "intent_translator",
        "planner"
      ],
      "fallback_version": "v1",
      "write_compiled_artifacts": true
    },
    "flow_control": {
      "default_tier": "standard",
      "auto_tier_by_change": true,
      "required_agents": {
        "lean": [
          "architect",
          "implementer",
          "qa_reviewer",
          "docs_writer"
        ],
        "standard": [
          "architect",
          "planner",
          "implementer",
          "qa_reviewer",
          "docs_writer"
        ],
        "strict": [
          "architect",
          "planner",
          "implementer",
          "qa_reviewer",
          "security_reviewer",
          "docs_writer",
          "release_manager"
        ]
      },
      "strict_triggers": [
        "dependency_files_changed",
        "build_or_dev_scripts_changed",
        "api_or_schema_changed",
        "auth_or_security_changed",
        "architecture_or_adr_changed"
