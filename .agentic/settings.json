      "batch_startup_logging": true
    },
    "approvals": {
      "auto_approve_in_agentx": true
    },
    "telemetry": {
      "enabled": true,
      "capture_tokens": true,
      "events": true,
      "questions": true,
      "questions_log": true
    },
    "automation": {
      "run_scripts": true,
      "auto_start_run": true,
      "auto_log_questions": true,
      "auto_log_agents": true
