{
  "Managed-By": "AgenticRepoBuilder",
  "Template-Source": "templates/.agentic/settings.json",
  "Template-Version": "2.3.0",
  "Last-Generated": "2026-02-06T16:43:23Z",
  "Ownership": "Managed",
  "settings": {
    "prd_intake": {
      "detect_without_keyword": true,
      "min_structural_signals": 3,
      "structural_signals": [
        "PROJECT:",
        "ROLE",
        "PRIMARY GOAL",
        "INFORMATION ARCHITECTURE",
        "REQUIREMENTS",
        "OUTPUT EXPECTATION"
      ],
      "on_ambiguity": "confirm_once",
      "update_strategy": "incremental",
      "create_new_prd_on_scope_reset": true,
      "write_prd_versions": true
    },
    "run_mode": {
      "ask_on_prd": true,
      "preferred": "AgentX",
      "default_if_unanswered": "AgentL"
    },
    "startup": {
      "profile": "fast",
      "max_initial_questions": 3,
      "ask_only_missing": true,
      "avoid_script_reads": true,
      "defer_repo_map": true,
      "single_calibration_message": true,
      "batch_startup_logging": true
    },
    "approvals": {
      "auto_approve_in_agentx": true
    },
    "telemetry": {
      "enabled": true,
      "capture_tokens": true,
      "events": true,
      "questions": true,
      "questions_log": true
    },
    "automation": {
      "run_scripts": true,
      "auto_start_run": true,
      "auto_log_questions": true,
      "auto_log_agents": true
    },
    "prompt_resolution": {
      "default_version": "v1",
      "pilot_v2_enabled": true,
      "pilot_agents": [
        "god_orchestrator",
        "intent_translator",
        "planner"
      ],
      "fallback_version": "v1",
      "write_compiled_artifacts": true
    },
    "flow_control": {
      "default_tier": "standard",
      "auto_tier_by_change": true,
      "required_agents": {
        "lean": [
          "architect",
          "implementer",
          "qa_reviewer",
          "docs_writer"
        ],
        "standard": [
          "architect",
          "planner",
          "implementer",
          "qa_reviewer",
          "docs_writer"
        ],
        "strict": [
          "architect",
          "planner",
          "implementer",
          "qa_reviewer",
          "security_reviewer",
          "docs_writer",
          "release_manager"
        ]
      },
      "strict_triggers": [
        "dependency_files_changed",
        "build_or_dev_scripts_changed",
        "api_or_schema_changed",
        "auth_or_security_changed",
        "architecture_or_adr_changed"
      ],
      "max_parallel_agents": 2
    },
    "agent_dispatch": {
      "mode": "hybrid",
      "evaluate_all_agents": true,
      "catalog": [
        "god_orchestrator",
        "intent_translator",
        "context_curator",
        "stack_advisor",
        "architect",
        "planner",
        "implementer",
        "qa_reviewer",
        "security_reviewer",
        "docs_writer",
        "release_manager",
        "repo_maintainer",
        "template_librarian",
        "migration_manager"
      ],
      "always_required_agents": [
        "architect",
        "qa_reviewer",
        "docs_writer"
      ],
      "signal_weights": {
        "file_match": 2,
        "prd_keyword": 1,
        "diff_hint": 2,
        "history_hint": 1
      },
      "trigger_detection": {
        "file_patterns": true,
        "prd_keywords": true,
        "git_diff_analysis": true
      },
      "conditional_agents": {
        "stack_advisor": {
          "tier_min": "standard",
          "min_score": 2
        },
        "architect": {
          "tier_min": "lean",
          "min_score": 1
        },
        "security_reviewer": {
          "tier_min": "standard",
          "min_score": 2
        },
        "migration_manager": {
          "tier_min": "standard",
          "min_score": 2
        },
        "repo_maintainer": {
          "tier_min": "lean",
          "min_score": 2
        },
        "template_librarian": {
          "tier_min": "standard",
          "min_score": 2
        }
      }
    },
    "checks": {
      "preflight_enabled": true,
      "preflight_run_install": true,
      "preflight_run_dev": true,
      "preflight_timeout_sec": 20,
      "package_manager_auto": true
    },
    "docs": {
      "project_runbook_path": "<project_root>/RUNBOOK.md"
    },
    "project_meta": {
      "enforce_compatibility": true,
      "required_files": [
        "AGENTS.md",
        "UNIVERSAL.md",
        "settings.json"
      ],
      "min_template_versions": {
        "AGENTS.md": "2.2.0",
        "UNIVERSAL.md": "1.4.0",
        "settings.json": "2.3.0"
      }
    },
    "run_start": {
      "enabled": true,
      "write_run_meta": true
    },
    "headless": {
      "default_run_mode": "AgentL"
    },
    "calibration_questions": {
      "min": 1,
      "max": 7
