        "stack_advisor": {
          "tier_min": "standard",
          "min_score": 2,
          "file_patterns": [
            "package.json",
            "package-lock.json",
            "pnpm-lock.yaml",
            "yarn.lock",
            "requirements.txt",
            "poetry.lock",
            "Cargo.toml",
            "Cargo.lock"
          ],
          "prd_keywords": [
            "dependency",
            "library",
            "framework",
            "upgrade",
            "version",
            "package"
          ]
        },
        "architect": {
          "tier_min": "lean",
          "min_score": 1,
          "file_patterns": [
