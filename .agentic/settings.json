            "auth",
            "security",
            "permission",
            "token",
            "password",
            "encryption"
          ]
        },
        "migration_manager": {
          "tier_min": "standard",
          "min_score": 2,
          "file_patterns": [
            ".agentic/migrations/"
          ],
          "prd_keywords": [
            "breaking change",
            "migration",
            "deprecated",
            "upgrade to"
          ]
        },
        "repo_maintainer": {
          "tier_min": "lean",
          "min_score": 2,
          "file_patterns": [
            "rename",
            "move",
            "cleanup",
            "structure"
          ],
          "prd_keywords": [
            "cleanup",
            "rename",
            "move",
            "debt",
            "restructure"
          ]
        },
        "template_librarian": {
          "tier_min": "standard",
          "min_score": 2,
          "file_patterns": [
            "templates/",
            "scaffold",
            "generator"
          ],
          "prd_keywords": [
            "template",
            "boilerplate",
            "generator",
            "scaffold"
          ]
        }
      }
