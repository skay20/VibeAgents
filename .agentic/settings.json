    "rollout": {
      "phase": "C",
      "enforcement_mode": "blocking",
      "report_only_runs_remaining": 0,
      "fallback_mode": "tier_only",
      "fallback_active": false
    },
    "checks": {
      "preflight_enabled": true,
      "preflight_run_install": true,
      "preflight_run_dev": true,
      "preflight_timeout_sec": 20,
      "package_manager_auto": true
    },
    "project_meta": {
      "enforce_compatibility": true,
      "required_files": [
        "AGENTS.md",
        "UNIVERSAL.md",
        "settings.json"
