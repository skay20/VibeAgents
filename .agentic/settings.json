{
  "Managed-By": "AgenticRepoBuilder",
  "Template-Source": "templates/.agentic/settings.json",
  "Template-Version": "2.3.0",
  "Last-Generated": "2026-02-06T16:43:23Z",
  "Ownership": "Managed",
  "settings": {
    "prd_intake": {
      "detect_without_keyword": true,
      "min_structural_signals": 3,
      "structural_signals": [
        "PROJECT:",
        "ROLE",
        "PRIMARY GOAL",
        "INFORMATION ARCHITECTURE",
        "REQUIREMENTS",
        "OUTPUT EXPECTATION"
      ],
      "on_ambiguity": "confirm_once",
      "update_strategy": "incremental",
      "create_new_prd_on_scope_reset": true,
      "write_prd_versions": true
    },
    "run_mode": {
      "ask_on_prd": true,
      "preferred": "AgentX",
      "default_if_unanswered": "AgentL"
    },
    "startup": {
      "profile": "fast",
      "official_entrypoint": "scripts/orchestrator-first.sh",
      "max_initial_questions": 3,
      "ask_only_missing": true,
      "avoid_script_reads": true,
      "defer_repo_map": true,
      "single_calibration_message": true,
      "batch_startup_logging": true
    },
    "approvals": {
      "auto_approve_in_agentx": true
    },
    "telemetry": {
      "enabled": true,
      "capture_tokens": true,
      "events": true,
      "questions": true,
      "questions_log": true
    },
    "automation": {
      "run_scripts": true,
      "auto_start_run": true,
      "auto_log_questions": true,
      "auto_log_agents": true
